{
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 6
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "rules": {
        "comma-dangle": ["error", "never"],
        "no-unused-vars": ["error", {
            "vars": "local",
            "args": "none"
        }],
        "no-console": "off"
    }
}